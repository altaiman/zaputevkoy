//- includes
include components/head/index.pug
include components/common/index.pug
include components/svg/index.pug

include components/header/index.pug
include components/breadcrumbs/index.pug

include components/footer/index.pug
include components/footer/scripts.pug

//- end includes

//- variables page 
- 
	pageSettings = {
		title: 'Главная'
	}

doctype html
html(lang=global.lang)
	+head(pageSettings)
	+b.BODY.page
		+header({ search: false })
		
		+e.MAIN.content(role="main")
			+b.SECTION.section
				+e.container.container
					+breadcrumbs(['Страницы вёрстки'])
			
					+b.SPAN.title Страницы вёрстки
					
					+b.items._quadruple
						+b.A.link(href="index.html" target="_blank") Главная
						+b.A.link(href="search-tour.html" target="_blank") Поиск туров
						+b.A.link(href="hotel.html" target="_blank") Страница отеля
						+b.A.link(href="reviews.html" target="_blank") Отзывы
						+b.A.link(href="contacts.html" target="_blank") Контакты
						+b.A.link(href="online-payment.html" target="_blank") Онлайн оплата - 1
						+b.A.link(href="online-payment2.html" target="_blank") Онлайн оплата - 2
						+b.A.link(href="tours.html" target="_blank") Горящие туры
						+b.A.link(href="loading.html" target="_blank") Пример загрузки страницы
						+b.A.link(href="achivments.html" target="_blank") Достижения
						+b.A.link(href="vacancies.html" target="_blank") Вакансии
						+b.A.link(href="requisites.html" target="_blank") Реквизиты
						+b.A.link(href="404-1.html" target="_blank") 404 - 1
						+b.A.link(href="404-2.html" target="_blank") 404 - 2
			
		+footer
		+scripts
